#usda 1.0
(
    doc = """Interface file for Material contribution"""
    defaultPrim = "campfire"
)

def "campfire" {
    def Scope "Materials" (
        prepend references = [@./rocks/material_rocks.usda@]
    )
    {
        def "MaterialBindings" {
            over "class_Rocks" { rel material:binding = </campfire/Materials/mat_Rocks> }
        }
    }
}
#usda 1.0
(
    doc = """Interface file for Material contribution"""
    defaultPrim = "campfire"
)

def "campfire"
(
    variants = {
            string material = "pbr"
        }
        prepend variantSets = "material"
)
{
    variantSet "material" = {
        "diffuse" (
            prepend payload = [@./diffuse/material_diffuse.usda@]
        )
        {
        }
        "pbr" (
            prepend payload = [@./pbr/material_pbr.usda@]
        )
        {
        }
    }
}